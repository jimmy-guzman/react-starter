{
  "name": "react-starter",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bunx --bun vite build",
    "check": "turbo format lint typecheck coverage build sb:build knip",
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "format": "prettier --check --cache .",
    "format:fix": "prettier --write --cache .",
    "knip": "knip",
    "lint": "eslint --color --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "bun lint --fix",
    "prepare": "is-ci || lefthook install",
    "preview": "vite preview",
    "sb": "storybook dev -p 6006",
    "sb:build": "storybook build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc"
  },
  "prettier": "@jimmy.codes/prettier-config",
  "dependencies": {
    "@tanstack/react-query": "5.64.2",
    "@tanstack/react-router": "1.97.3",
    "clsx": "2.1.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "tailwind-merge": "2.6.0"
  },
  "devDependencies": {
    "@iconify-json/logos": "1.2.4",
    "@iconify-json/lucide": "1.2.24",
    "@iconify/tailwind": "1.2.0",
    "@jimmy.codes/eslint-config": "3.29.0",
    "@jimmy.codes/prettier-config": "1.3.0",
    "@playwright/test": "1.49.1",
    "@storybook/addon-a11y": "8.5.0",
    "@storybook/addon-essentials": "8.5.0",
    "@storybook/addon-interactions": "8.5.0",
    "@storybook/addon-links": "8.5.0",
    "@storybook/addon-themes": "8.5.0",
    "@storybook/react": "8.5.0",
    "@storybook/react-vite": "8.5.0",
    "@tailwindcss/typography": "0.5.16",
    "@tanstack/react-query-devtools": "5.64.2",
    "@tanstack/router-devtools": "1.97.3",
    "@tanstack/router-vite-plugin": "1.97.3",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@testing-library/user-event": "14.6.0",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/bun": "1.1.17",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "@vitejs/plugin-react-swc": "3.7.2",
    "@vitest/coverage-v8": "3.0.2",
    "@vitest/ui": "3.0.2",
    "autoprefixer": "10.4.20",
    "daisyui": "4.12.23",
    "eslint": "9.18.0",
    "gitzy": "5.7.1",
    "happy-dom": "16.6.0",
    "is-ci": "4.1.0",
    "jiti": "2.4.2",
    "knip": "5.42.2",
    "lefthook": "1.10.8",
    "msw": "2.7.0",
    "postcss": "8.5.1",
    "prettier": "3.4.2",
    "storybook": "8.5.0",
    "tailwindcss": "3.4.17",
    "turbo": "2.3.3",
    "typescript": "5.7.3",
    "vite": "6.0.7",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.2"
  },
