{
  "name": "react-starter",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bunx --bun vite build",
    "check": "turbo format lint typecheck coverage build knip",
    "coverage": "vitest run --coverage",
    "dev": "bunx --bun vite",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "format": "prettier --check --cache .",
    "format:fix": "prettier --write --cache .",
    "knip": "knip",
    "lint": "eslint --color --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "bun lint --fix",
    "prepare": "is-ci || lefthook install",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc"
  },
  "prettier": "@jimmy.codes/prettier-config",
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.0"
  },
  "dependencies": {
    "@tanstack/react-query": "5.85.5",
    "@tanstack/react-router": "1.131.27",
    "react": "19.1.1",
    "react-dom": "19.1.1"
  },
  "devDependencies": {
    "@iconify-json/logos": "1.2.9",
    "@iconify-json/lucide": "1.2.64",
    "@iconify/tailwind4": "1.0.6",
    "@jimmy.codes/eslint-config": "6.6.0",
    "@jimmy.codes/prettier-config": "2.0.2",
    "@playwright/test": "1.55.0",
    "@tailwindcss/typography": "0.5.16",
    "@tailwindcss/vite": "4.1.12",
    "@tanstack/react-query-devtools": "5.85.5",
    "@tanstack/react-router-devtools": "1.131.28",
    "@tanstack/router-vite-plugin": "1.131.28",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/bun": "1.2.21",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.8",
    "@vitejs/plugin-react": "5.0.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "babel-plugin-react-compiler": "19.1.0-rc.2",
    "daisyui": "5.0.53",
    "eslint": "9.34.0",
    "gitzy": "5.9.0",
    "happy-dom": "18.0.1",
    "is-ci": "4.1.0",
    "jiti": "2.5.1",
    "knip": "5.63.0",
    "lefthook": "1.12.3",
    "msw": "2.10.5",
    "prettier": "3.6.2",
    "tailwindcss": "4.1.12",
    "turbo": "2.5.6",
    "typescript": "5.9.2",
    "vite": "npm:rolldown-vite@7.1.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "packageManager": "bun@1.2.19",
  "trustedDependencies": [
    "@tailwindcss/oxide",
    "oxc-resolver",
    "unrs-resolver"
  ]
}
