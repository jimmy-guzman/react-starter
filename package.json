{
  "name": "react-starter",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bunx --bun vite build",
    "check": "turbo format lint typecheck coverage build sb:build knip",
    "coverage": "vitest run --coverage",
    "dev": "bunx --bun vite",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "format": "prettier --check --cache .",
    "format:fix": "prettier --write --cache .",
    "knip": "knip",
    "lint": "eslint --color --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "bun lint --fix",
    "prepare": "is-ci || lefthook install",
    "preview": "vite preview",
    "sb": "storybook dev -p 6006",
    "sb:build": "storybook build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc"
  },
  "prettier": "@jimmy.codes/prettier-config",
  "overrides": {
    "vite": "npm:rolldown-vite@7.0.11"
  },
  "dependencies": {
    "@radix-ui/react-slot": "1.2.3",
    "@tanstack/react-query": "5.84.0",
    "@tanstack/react-router": "1.130.9",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "tailwind-merge": "3.3.1"
  },
  "devDependencies": {
    "@iconify-json/logos": "1.2.5",
    "@iconify-json/lucide": "1.2.60",
    "@iconify/tailwind4": "1.0.6",
    "@jimmy.codes/eslint-config": "6.1.0",
    "@jimmy.codes/prettier-config": "1.6.0",
    "@playwright/test": "1.54.2",
    "@storybook/addon-a11y": "9.1.0",
    "@storybook/addon-docs": "^9.1.0",
    "@storybook/addon-links": "9.1.0",
    "@storybook/addon-themes": "9.1.0",
    "@storybook/react-vite": "9.1.0",
    "@tailwindcss/typography": "0.5.16",
    "@tailwindcss/vite": "4.1.11",
    "@tanstack/react-query-devtools": "5.84.1",
    "@tanstack/react-router-devtools": "1.130.13",
    "@tanstack/router-vite-plugin": "1.130.15",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.6.4",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/bun": "1.2.19",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "@vitejs/plugin-react-oxc": "0.3.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "daisyui": "5.0.50",
    "eslint": "9.32.0",
    "gitzy": "5.9.0",
    "happy-dom": "18.0.1",
    "is-ci": "4.1.0",
    "jiti": "2.5.1",
    "knip": "5.62.0",
    "lefthook": "1.12.2",
    "msw": "2.10.4",
    "prettier": "3.6.2",
    "storybook": "9.1.0",
    "tailwindcss": "4.1.11",
    "turbo": "2.5.5",
    "typescript": "5.8.3",
    "vite": "npm:rolldown-vite@7.0.11",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "packageManager": "bun@1.2.19",
  "trustedDependencies": [
    "@tailwindcss/oxide",
    "unrs-resolver"
  ]
}
