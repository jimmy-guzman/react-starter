{
  "name": "react-starter",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "turbo format lint typecheck coverage build knip spelling",
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "format": "prettier --check --cache --experimental-cli .",
    "format:fix": "prettier --write --cache --experimental-cli .",
    "knip": "knip",
    "lint": "eslint --color --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "pnpm lint --fix",
    "prepare": "is-ci || lefthook install",
    "preview": "vite preview",
    "spelling": "cspell --config cspell.config.ts \"**/*\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@tanstack/react-query": "5.90.19",
    "@tanstack/react-router": "1.151.6",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@iconify-json/logos": "1.2.10",
    "@iconify-json/lucide": "1.2.81",
    "@iconify/tailwind4": "1.2.0",
    "@jimmy.codes/eslint-config": "6.26.0",
    "@playwright/test": "1.57.0",
    "@tailwindcss/typography": "0.5.19",
    "@tailwindcss/vite": "4.1.18",
    "@tanstack/devtools-vite": "0.3.12",
    "@tanstack/eslint-plugin-router": "1.141.0",
    "@tanstack/react-devtools": "0.8.4",
    "@tanstack/react-query-devtools": "5.91.1",
    "@tanstack/react-router-devtools": "1.141.4",
    "@tanstack/router-vite-plugin": "1.141.5",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@testing-library/user-event": "14.6.1",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vite-pwa/assets-generator": "1.0.2",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/coverage-v8": "4.0.16",
    "@vitest/ui": "4.0.16",
    "babel-plugin-react-compiler": "1.0.0",
    "cspell": "9.4.0",
    "daisyui": "5.5.14",
    "eslint": "9.39.2",
    "gitzy": "6.5.7",
    "happy-dom": "20.0.11",
    "is-ci": "4.1.0",
    "jiti": "2.6.1",
    "knip": "5.74.0",
    "lefthook": "2.0.12",
    "msw": "2.12.4",
    "prettier": "3.7.4",
    "prettier-plugin-packagejson": "2.5.20",
    "prettier-plugin-tailwindcss": "0.7.2",
    "tailwindcss": "4.1.18",
    "turbo": "2.6.3",
    "typescript": "5.9.3",
    "vite": "npm:rolldown-vite@7.2.11",
    "vite-plugin-pwa": "1.2.0",
    "vite-tsconfig-paths": "6.0.1",
    "vitest": "4.0.16"
  },
  "packageManager": "pnpm@10.26.0"
}
